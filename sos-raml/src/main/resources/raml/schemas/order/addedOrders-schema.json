      "items": {
        "$ref": "orderPath200-schema.json"
      }
    },
    "ok": {
      "type": "boolean"
    },
    "errors": {
      "type": "array",
      "items": {
        "$ref": "../common/error419-schema.json"
      }
    }
  }
