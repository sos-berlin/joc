{
  "title": "task filter",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.job.TaskFilter",
  "type": "object",
  "required": [
    "jobschedulerId"
  ],
  "properties": {
    "jobschedulerId": {
      "$ref": "../common/filename100-schema.json"
    },
    "taskId": {
      "type": "string",
      "pattern": "^\\d+$"
    },
    "filename": {
      "$ref": "../common/filename255-schema.json"
    },
    "mime": {
      "$ref": "../common/logMimeFilter-schema.json"
    }
  }
}
