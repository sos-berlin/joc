      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "path": {
      "$ref": "../common/path-schema.json"
    },
    "name": {
      "type": "string"
    },
    "basedOn": {
      "$ref": "../common/path-schema.json"
    },
    "documentation": {
      "$ref": "../common/path-schema.json"
    },
    "type": {
      "$ref": "calendarType-schema.json"
    },
    "category": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "from": {
      "$ref": "../common/date-schema.json"
    },
    "to": {
      "$ref": "../common/date-schema.json"
    },
    "periods": {
      "type": "array",
