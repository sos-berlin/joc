{
  "title": "order plan filter",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.plan.RunTimePlanFilter",
  "type": "object",
  "required": [
    "jobschedulerId",
    "runTime",
    "dateTo"
  ],
  "properties": {
    "jobschedulerId": {
      "$ref": "../common/filename100-schema.json"
    },
    "runTime": {
      "$ref": "../joe/schedule/runTime-schema.json"
    },
    "dateFrom": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "dateTo": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    }
  }
}
