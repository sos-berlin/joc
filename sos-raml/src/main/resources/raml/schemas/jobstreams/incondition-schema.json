{
	"title": "In-Condition",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "In Condition",
	"javaType": "com.sos.joc.model.jobstreams.InCondition",
	"type": "object",
	"required": [
		"deliveryDate"
	],
	"properties": {
		"id": {
			"$ref": "../common/nonNegativeLong-schema.json"
		},
		"jobStream": {
			"type": "string"
		},
		"consumed": {
			"type": "boolean"
		},
		"markExpression": {
			"type": "boolean"
		},
		"skipOutCondition": {
			"type": "boolean"
		},
		"conditionExpression": {
			"$ref": "condition_expression-schema.json"
		},
		"nextPeriod": {
			"$ref": "../common/deliveryDate-schema.json"
		},
		"inconditionCommands": {
			"type": "array",
			"items": {
				"$ref": "incondition_command-schema.json"
			}
		},
		"outconditions": {
			"type": "array",
			"items": {
				"$ref": "jobstreamConditions-schema.json"
			}
		}
	}
}
