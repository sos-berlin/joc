    },
    "orderId": {
      "type": "string"
    },
    "jobChain": {
      "$ref": "../common/path-schema.json"
    },
    "startTime": {
      "$ref": "../common/timestamp-schema.json"
    },
    "endTime": {
      "$ref": "../common/timestamp-schema.json"
    },
    "node": {
      "type": "string"
    },
    "state": {
      "$ref": "../common/historyState-schema.json"
    },
    "historyId": {
      "type": "string"
    },
    "children": {
