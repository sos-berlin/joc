{
  "title": "start job command",
  "id": "schemas/joe/job/startJob",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.joe.job.StartJob",
  "type": "object",
  "properties": {
    "job": {
      "type": "string"
    },
    "at": {
      "type": "string",
      "default": "now"
    },
    "after": {
      "type": "string"
    },
    "force": {
      "description": "possible values: yes, no, 1, 0, true, false",
      "type": "string"
    },
    "params": {
      "$ref": "../common/params-schema.json"
    },
    "environment": {
      "$ref": "environment-schema.json"
    }
  }
}
