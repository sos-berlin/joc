{
  "title": "add order",
  "id": "schemas/joe/order/addOrder",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.joe.order.AddOrder",
  "type": "object",
  "extends" : {
    "javaType": "Order"
  },
  "required": [
    "jobChain"
  ],
  "properties": {
    "jobChain": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "at": {
      "type": "string"
    },
    "replace": {
      "description": "possible values: yes, no, 1, 0, true, false",
      "type": "string"
    },
    "suspended": {
      "description": "possible values: yes, no, 1, 0, true, false",
      "type": "string"
    },
    "runTime": {
      "$ref": "../schedule/runTime-schema.json"
    }
  }
}
