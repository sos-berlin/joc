{
	"title": "folder content",
	"id": "schemas/joe/other/folder",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.joe.other.Folder",
	"type": "object",
	"required": [
		"path"
	],
	"properties": {
		"path": {
			"$ref": "../../common/path-schema.json"
		},
		"folders": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"jobs": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"jobChains": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"orders": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"processClasses": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"locks": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"schedules": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"monitors": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"params": {
		    "description": "external parameter files for jobs and orders",
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"nodeParams": {
			"description": "[jobChainName].config.xml files with node parameters",
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"holidays": {
			"description": "external holidays files",
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"others": {
			"description": "external files, e.g. shell scripts to include in job's script",
			"type": "array",
			"items": {
				"type": "string"
			}
		}
	}
}
