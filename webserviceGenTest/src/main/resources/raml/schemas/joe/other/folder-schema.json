{
	"title": "folder content",
	"id": "schemas/joe/other/folder",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.joe.other.Folder",
	"type": "object",
	"required": [
		"deliveryDate",
		"path"
	],
	"properties": {
		"deliveryDate": {
			"$ref": "../../common/deliveryDate-schema.json"
		},
		"path": {
			"$ref": "../../common/path-schema.json"
		},
		"_message": {
			"type": "string"
		},
		"folders": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"jobs": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"jobChains": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"orders": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"processClasses": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"locks": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"schedules": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"monitors": {
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"nodeParams": {
			"description": "[jobChainName].config.xml files with node parameters",
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		},
		"others": {
			"description": "external files, e.g. shell scripts to include in job's script, parameter files for jobs and orders, holidays files",
			"type": "array",
			"items": {
				"$ref": "folderItem-schema.json"
			},
			"uniqueItems": true
		}
	}
}
